{
  "name": "hatool",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve hatool-tester",
    "lint": "ng lint",
    "build_lib": "ng build hatool --configuration production",
    "watch_lib": "ng build hatool --watch",
    "copy-license": "cp ./LICENSE ./dist/hatool",
    "copy-readme": "cp ./README.md ./dist/hatool",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/hatool && npm pack",
    "package": "npm run build_lib && npm run copy-files",
    "publish": "npm run package && cd dist/hatool && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "highlight.js": "^10.4.1",
    "ngx-highlightjs": "^4.1.2",
    "rxjs": "~6.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~12.2.15",
    "@angular/compiler-cli": "~12.2.16",
    "@types/highlight.js": "^9.12.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.6",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}